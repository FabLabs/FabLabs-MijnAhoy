<div id="jqt">
	
	<div id="cover" class="current">
		<a class="button slideup reverse" id="infoButton" a href="#home">Sluit</a>
		<a class="slideup reverse" href="#home"><img src="/public/images/cover.png"/></a>
	</div>
	
	<div id="home" >
		<div class="toolbar">
		<a class="button leftButton slideup"  href="#new">New</a>
		<a class="button slideup" id="infoButton" href="#cover">Cover</a>
			<h1>Mijn Ahoy</h1>	
		</div>
		
		<div class="vertical-scroll">
			<div>
    		<ul class="plastic">
			
        		<% @mijnahoys.each do |obj| %>
            
				<li class="arrow">
					<a href="<%= url_for :action => :show, :id => obj.object %>">
						<span class="title"><%= obj.titel %></span>
						<span class="disclosure_indicator"></span>
					</a>
				</li>
            
        			<% end %>
    		</ul>
			</div>
		</div>
	</div>	

	<div id="new" >
	  <div class="toolbar">
		<a href="#home" class="button leftButton slideup reverse">Cancel</a>
		<span class="story_name" style="text-align:center;top:7px"><h1>Add Story</h1></span>
	  </div>
	  		<ul id="ticket" class="project">
	
	
				<li>
				  <a href="/app/Mijnahoy/new" class="project">
					<div style="float:left;width:235px;">
					  <small style="font-size:14px;"></small>
					  <span class="story_name">Add new feature</span>
					</div>
				  </a>
				</li>
	
			
	  		</ul>
	</div>
	

</div>