<div id="jqt">
	
	<div id="cover" class="current">
		<a class="button slideup reverse" id="infoButton" a href="#home">Sluit</a>
		<a class="slideup reverse" href="#home"><img src="/public/images/cover.png"/></a>
	</div>
	
	<div id="home" >
		<div class="toolbar"><a class="button slideup" id="infoButton" a href="#cover">Cover</a>
			<h1>Mijn Ahoy</h1>	
		</div>
		<div class="vertical-scroll">
			<div>    
    		<ul class="plastic">
        		<% @mijnahoys.each do |obj| %>
            
				<li  class="arrow">
					 <a  href="/app/MijnAhoy/<%=  obj.object %>/show?">
						<span class="title"><%= obj.titel %></span>
						<span class="disclosure_indicator"></span>
					</a>
				</li  class="arrow">
            
        			<% end %>
					<div id="rightItem" class="regularButton"><%= link_to "New", :controller => :MijnAhoy, :action => :new %></div>
    		</ul>
			</div>
		</div>
	</div>


</div>